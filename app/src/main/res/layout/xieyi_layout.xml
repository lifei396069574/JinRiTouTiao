<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <ImageView

            android:layout_margin="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/xianyi_imagebutton1"
            android:src="@mipmap/back_discover_normal"
            />
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="今日头条用户协议"
            android:textSize="25sp"
            android:id="@+id/zhuce_textview1"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="5dp"

            />
        <ImageView
            android:layout_alignParentRight="true"
            android:background="@color/colorWrite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/xianyi_imagebutton2"
            android:src="@mipmap/btn_more_title_detail_normal"
            android:layout_marginLeft="50dp"
            android:layout_margin="20dp"
            />
    </RelativeLayout>

        <WebView
            android:id="@+id/xianyi_webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            ></WebView>


    </LinearLayout>
</RelativeLayout>
